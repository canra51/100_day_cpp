#include <iostream>
using namespace std;

int main() {
    int x = 10;
    int y = 5;

    // Operator perbandingan
    cout << "x == y : " << (x == y) << endl;  // false
    cout << "x != y : " << (x != y) << endl;  // true
    cout << "x > y : " << (x > y) << endl;    // true
    cout << "x < y : " << (x < y) << endl;    // false
    cout << "x >= y : " << (x >= y) << endl;  // true
    cout << "x <= y : " << (x <= y) << endl;  // false

    return 0;
}

